<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find panda_four_circle_moveit_config)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="panda_zero_arm_position_controller panda_one_arm_position_controller panda_two_arm_position_controller panda_three_arm_position_controller panda_multi_arm_position_controller "/>

</launch>
